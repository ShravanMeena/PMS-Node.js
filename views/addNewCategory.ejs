<%- include('header') %>



<div class="container mt-4">

<!-- Default form login -->
<form class="text-center border border-light p-5  col-lg-4 offset-lg-4" action="add-new-category" method="POST">

<h5>Add New Password Category</h5>
<%if(errors !=''){%>
  <p style="color: red;"> <%= errors.passwordCategory.msg %> </p>
<%}%>  
<%if(success !=''){%>
  <p style="color: green;"> <%= success %> </p>
<%}%>  
    <!-- Email -->
    <input type="text" name="passwordCategory" class="form-control mb-4" placeholder="Enter Password Category">

   
    <!-- Sign in button -->
    <button class="btn btn-info btn-block my-4" type="submit">Add</button>

    <!-- Register -->
    <p>
        <a href="">View All Category Lists</a>
    </p>

 
</form>
<!-- Default form login -->

</div>

<%- include('footer') %>
